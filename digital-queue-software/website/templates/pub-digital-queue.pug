extends ./_pub-config-base.pug


//- Template variables
block append vars
    - var $templateName = "pub-digital-queue"
    - var $templateClass = "pub-digital-queue"


//- Page styles
block append styles
    link(rel="stylesheet", href=`/css/${$templateName}.css`)/


//- Main Content
block append content
