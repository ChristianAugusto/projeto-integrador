extends ./_configbase.pug


//- Template variables
block append vars
    - var $templateName = "pub-home"
    - var $templateClass = "pub-home"


//- Page styles
block append styles
    link(rel="stylesheet", href=`/css/${$templateName}.css`)/


//- Main Content
block append content
    include ./partials/pub/home/intro.pug

//- Page scripts
block append scripts
    script(src=`/js/${$templateName}.js`)