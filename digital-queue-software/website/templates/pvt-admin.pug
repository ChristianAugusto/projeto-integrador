extends ./_pvt-config-base.pug


//- Template variables
block append vars
    - var $templateName = "pvt-admin"
    - var $templateClass = "pvt-admin"


//- Page styles
block append styles
    link(rel="stylesheet", href=`/css/${$templateName}.css`)/


//- Main Content
block append content
    include ./partials/pvt/admin/intro.pug

//- Page scripts
block append scripts
    script(src=`/js/${$templateName}.js`)